{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "punk-fluffy"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "scss/preloader.css",
            "scss/vendors/by_hands/bootstrap/css/bootstrap.min.css",
            "scss/vendors/by_bower/jscrollpane/style/jquery.jscrollpane.css",
            "scss/vendors/by_bower/ladda/dist/ladda-themeless.min.css",
            "scss/vendors/by_bower/bootstrap-select/dist/css/bootstrap-select.min.css",
            "scss/vendors/by_bower/select2/dist/css/select2.min.css",
            "scss/vendors/by_bower/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css",
            "scss/vendors/by_bower/fullcalendar/dist/fullcalendar.min.css",
            "scss/vendors/by_bower/bootstrap-sweetalert/dist/sweetalert.css",
            "scss/vendors/by_bower/summernote/dist/summernote.css",
            "scss/vendors/by_bower/owl.carousel/dist/assets/owl.carousel.min.css",
            "scss/vendors/by_bower/ionrangeslider/css/ion.rangeSlider.css",
            "scss/vendors/by_bower/datatables/media/css/dataTables.bootstrap4.min.css",
            "scss/vendors/by_bower/c3/c3.min.css",
            "scss/vendors/by_bower/chartist/dist/chartist.min.css",
            "scss/vendors/by_bower/nprogress/nprogress.css",
            "scss/vendors/by_bower/jquery-steps/demo/css/jquery.steps.css",
            "scss/vendors/by_bower/dropify/dist/css/dropify.min.css",
            "scss/vendors/by_hands/font-linearicons/style.css",
            "scss/vendors/by_hands/font-icomoon/style.css",
            "scss/vendors/by_hands/font-awesome/css/font-awesome.min.css",
            "scss/vendors/by_hands/cleanhtmlaudioplayer/src/player.css",
            "scss/vendors/by_hands/cleanhtmlvideoplayer/src/player.css",
            "scss/modules/core/common/core.cleanui.css",
            "scss/modules/vendors/common/vendors.cleanui.css",
            "scss/modules/layouts/common/layouts-pack.cleanui.css",
            "scss/modules/themes/common/themes.cleanui.css",
            "scss/modules/menu-left/common/menu-left.cleanui.css",
            "scss/modules/menu-right/common/menu-right.cleanui.css",
            "scss/modules/top-bar/common/top-bar.cleanui.css",
            "scss/modules/footer/common/footer.cleanui.css",
            "scss/modules/pages/common/pages.cleanui.css",
            "scss/modules/ecommerce/common/ecommerce.cleanui.css",
            "scss/modules/apps/common/apps.cleanui.css",
            "styles.scss"
        ],
        "scripts": [],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
            "project": "src/tsconfig.app.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "src/tsconfig.spec.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "e2e/tsconfig.e2e.json",
            "exclude": "**/node_modules/**"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "scss",
        "component": {}
    }
}